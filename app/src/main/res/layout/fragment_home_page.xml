<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/homeRecyclerView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/no_layout_width_textview"
        android:layout_marginLeft="@dimen/no_layout_width_textview"
        android:layout_marginTop="@dimen/no_layout_width_textview"
        android:layout_marginEnd="@dimen/no_layout_width_textview"
        android:layout_marginRight="@dimen/no_layout_width_textview"
        android:layout_marginBottom="@dimen/no_layout_width_textview"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@color/una_paper"/>
</androidx.constraintlayout.widget.ConstraintLayout>